{
  "manifest_version": 3,
  "name": "Add Google Maps Button",
  "version": "1.0",
    "description": "Adds a Maps button to the Google search bar.",
      "permissions": [],

  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_icon": "icon128.png"
  }
}
